{
  "options": {
    "beautify": true
  },
  "files": [
    {
      "filename": "./github.json",
      "output": "./github.ts",
      "options": {
        "renameTypes": {
          "BigInt": "_BigInt"
        },
        "scalarTypes": {
          "_BigInt": "string",
          "URI": "string",
          "Date": "string",
          "DateTime": "string",
          "HTML": "string",
          "GitObjectID": "string",
          "GitTimestamp": "string",
          "Base64String": "string",
          "GitSSHRemote": "string",
          "PreciseDateTime": "string",
          "X509Certificate": "string",
          "GitRefname": "string"
        }
      }
    }
  ]
}
